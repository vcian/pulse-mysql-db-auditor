{
    "name": "vcian/pulse-mysql-db-auditor",
    "description": "A Laravel Pulse card show database auditing",
    "keywords": [
        "pulse",
        "laravel",
        "pulse-db-auditor",
        "pulse-mysql-db-auditor",
        "mysql-auditor",
        "mysql-standard"
    ],
    "homepage": "https://github.com/vcian/pulse-mysql-db-auditor",
    "license": "MIT",
    "authors": [
        {
            "name": "Vcian - ViitorCloud",
            "homepage": "https://github.com/vcian"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "*",
        "laravel/pulse": "^1.0.0@beta",
        "vcian/laravel-db-auditor": "^1.7.0"
    },
    "autoload": {
        "psr-4": {
            "Vcian\\Pulse\\PulseMysqlDBAuditor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vcian\\Pulse\\PulseMysqlDBAuditor\\PulseMysqlDBAuditorServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
